#!/usr/bin/env bash

# Check if the log file exists
if [ -f "apache-access.log" ]; then
    # Use grep, sort, and sed to display the desired output
    grep -oE "([0-9]{1,3}\.){3}[0-9]{1,3}" apache-access.log | sort | uniq -c | sed -E 's/^ *([0-9]+) (.+)/    \2 \1/'
else
    echo "Error: apache-access.log file not found."
fi
